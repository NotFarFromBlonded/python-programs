def check_armstrong(x):
    sum_dig = 0
    x_copy=x
    while x<>0:
        sum_dig = sum_dig +(x%10)**3
        x = x/10
        if sum_dig==x_copy:
            return True
        else:
            return False
def main():
    x = input ("enter a no.")
    if check_armstrong(x):
        print x ,"is an armstrong no."
    else:
        print x,"is not an angstrom no."
main()        

    

    

